Question : Binary Number with alternating bits
Time Complexity : O(logn)
Space Compexity : O(1)

--

bool hasAlternatingBits(int n){
     int prev=n%2;
     n=n/2;

     while(n>0){
         int curr=n%2;
         if(prev==curr)
         return false;

         prev=curr;
         n=n/2;
     }

     return true;
}
