Question : Generate Parenthesis
Time compelxity : O(4^n)/n^1/2
Space complexity : O(n)

vector<string> generateParenthesis(int n){
     string op="";
     vector<string> ans;
     solve(n,n,op,ans);
     return ans;
}

void solve(int open,int close,string op,vector<string>& ans){
     if(open==0 && close==0){
        ans.push_back(op);
        return;
     }

     if(open>0)
     solve(open-1,close,op+'(',ans);

     if(close>open)
     solve(open,close-1,op+')',ans);
}
